{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "jsx": "react",
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "types": ["@testing-library/jest-dom", "node", "jest"],
    "paths": { "@/*": ["src/*"], "@@/*": ["src/.umi/*"], "@rc-component/tree": ["src/index.ts"] }
  },
  "include": [
    ".dumirc.ts",
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./docs/**/*.tsx",
    "./tests/**/*.tsx"
  ]
}
