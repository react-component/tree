{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "jsx": "react",
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@@/*": [
        "src/.umi/*"
      ],
      "rc-tree": [
        "src/index.ts"
      ]
    }
  },
  "include": [
    ".dumirc.ts",
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./docs/**/*.tsx",
    "./tests/**/*.tsx",
  ]
}