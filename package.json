    "react-tree",
    "tree"
  ],
  "main": "lib/index",
  "files": [
    "assets/*.css",
    "lib"
  ],
  "homepage": "http://github.com/react-component/tree",
  "author": "hualei5280@gmail.com",
  "repository": {
    "type": "git",
