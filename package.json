{
  "name": "paragon-rc-tree",
  "version": "1.5.1",
  "description": "tree ui component for react",
  "keywords": [
    "react",
    "react-component",
    "react-tree",
    "tree"
  ],
  "main": "lib/index",
  "module": "./es/index",
  "files": [
    "assets/*.css",
    "assets/*.png",
    "assets/*.gif",
    "dist",
    "es",
    "lib"
  ],
  "entry": {
    "rc-tree": [
      "./assets/index.less",
      "./src/index.js"
    ]
  },
  "homepage": "http://github.com/davidboy/rc-tree",
  "author": "davidr@paragontruss.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:davidboy/rc-tree.git"
  },
  "bugs": {
    "url": "http://github.com/react-component/tree/issues"
  },
  "license": "MIT",
  "config": {
    "port": 8018
  },
  "scripts": {
    "build": "rc-tools run build",
    "dist": "rc-tools run dist",
    "compile": "rc-tools run compile --babel-runtime",
    "compile-noruntime": "rc-tools run compile",
    "gh-pages": "rc-tools run gh-pages",
    "start": "rc-tools run server",
    "pub": "rc-tools run pub --babel-runtime",
    "lint": "rc-tools run lint",
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*"
    ],
    "transform": {
      "\\.jsx?$": "./node_modules/rc-tools/scripts/jestPreprocessor.js"
    }
  },
  "devDependencies": {
    "coveralls": "^2.11.15",
    "css-animation": "^1.2.0",
    "enzyme": "^2.6.0",
    "enzyme-to-json": "^1.4.5",
    "es6-promise": "^3.1.2",
    "jest": "^18.0.0",
    "pre-commit": "1.x",
    "rc-dialog": "^6.1.0",
    "rc-tools": "^6.x",
    "rc-tooltip": "3.x",
    "rc-trigger": "^1.0.6",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-test-renderer": "^15.5.4"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "classnames": "2.x",
    "object-assign": "4.x",
    "prop-types": "^15.5.8",
    "rc-animate": "2.x",
    "rc-util": "^4.0.4"
  },
  "pre-commit": [
    "lint"
  ]
}
