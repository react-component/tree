    "@types/react-dom": "^16.8.4",
    "@types/warning": "^3.0.0",
    "css-animation": "^1.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.0.0",
    "eslint": "^7.0.0",
    "father": "^2.25.0-alpha.1",
    "less": "^3.11.1",
    "np": "^6.0.0",
    "rc-dialog": "^7.3.1",
    "rc-tooltip": "4.x",
    "rc-trigger": "^4.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.1",
    "classnames": "2.x",
